{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noEmit": false,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "plugins": [
    {
      "name": "next"
    }
  ],
  "paths": {
    "@core/*": [
      "./src/core/*"
    ],
    "@ui/*": [
      "./src/ui/*"
    ],
    "@lib/*": [
      "./lib/*"
    ],
    "@/*": [
      "./*"
    ],
    "@payload-config": [
      "./payload.config.ts"
    ]
  }
}
